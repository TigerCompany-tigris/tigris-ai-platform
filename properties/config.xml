<?xml version="1.0"?>
<properties>
    <nlp-handler package-path="util.nlp" method-name="nlp_twitter" />
    <data-handler>
        <word_pool package-path="util.data_handler" class-name="WordPool" />
        <word_filter package-path="util.data_handler" class-name="WordFilter" />
        <word_relationship package-path="util.data_handler" class-name="WordRelationship" />
    </data-handler>
    <api-handler>
        <api package-path="api.SearchTopics" class-name="SearchTopic" service-url="/searchTopic" />
        <api package-path="api.TopicCloud" class-name="TopicCloud" service-url="/topicCloud" />
        <api package-path="api.TopicNetwork" class-name="TopicNetwork" service-url="/topicNetwork" />
    </api-handler>
</properties>
